	component div is
		port (
			numer    : in  std_logic_vector(7 downto 0) := (others => 'X'); -- numer
			denom    : in  std_logic_vector(7 downto 0) := (others => 'X'); -- denom
			quotient : out std_logic_vector(7 downto 0);                    -- quotient
			remain   : out std_logic_vector(7 downto 0)                     -- remain
		);
	end component div;

